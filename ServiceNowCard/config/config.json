{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "my-incidents-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/myIncidents/MyIncidentsAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/myIncidents/MyIncidentsAdaptiveCardExtension.manifest.json"
        }
      ]
    },
    "create-incident-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/createIncident/CreateIncidentAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/createIncident/CreateIncidentAdaptiveCardExtension.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "MyIncidentsAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/myIncidents/loc/{locale}.js",
    "CreateIncidentAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/createIncident/loc/{locale}.js"
  }
}
